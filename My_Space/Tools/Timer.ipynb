   "source": [
    "import time\n",
    "\n",
    "secs = int(input('enter time in seconds : '))\n",
    "while secs:\n",
    "    mins, sec = divmod(secs, 60)\n",
    "    timef = '{:02d}:{:02d}'.format(mins,sec)\n",
    "    print(timef,end=\"\\r\")\n",
    "    time.sleep(1)\n",
    "    secs-=1\n",
    "\n",
    "print()\n",
    "print(\"Done!\")"
   ]
